{
  "compilerOptions": {
    "module": "commonjs",
    "downlevelIteration": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": true,
    "pretty": true,
    "esModuleInterop": true,
    "target": "esnext",
    "noEmit": true,
    "removeComments": true,
  }
}
